{
  "name": "braziljs",
  "version": "2.0.0",
  "description": "A non-profit foundation with a mission to move and unify JavaScript community in Brazil.",
  "engines": {
    "node": "0.10",
    "npm": "1.3"
  },
  "dependencies": {
    "docpad": "6.63",
    "docpad-plugin-cleanurls": "~2.5.2",
    "docpad-plugin-eco": "~2.0.2",
    "docpad-plugin-ghpages": "~2.4.0",
    "docpad-plugin-marked": "~2.1.2",
    "docpad-plugin-moment": "~2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/braziljs/foundation.git"
  },
  "scripts": {
    "deploy": "docpad deploy-ghpages --env static",
    "generate": "docpad generate --env static",
    "watch": "docpad run"
  },
  "devDependencies": {
    "grunt": "~0.4.2",
    "grunt-html-validation": "~0.1.13"
  }
}